class Fish {
    field String name;
    field int size;

    constructor Fish new(String n, int s) {
        let name = n;
        let size = s;
        return this;
    }

    method String getName() { return name; }
    method int getSize() { return size; }

    method void print() {
        do Output.printString(name);
        do Output.println();
        do output.printInt(size);
        return;
    }
}