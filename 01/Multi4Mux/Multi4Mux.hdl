    // And(a=c, b=sel[1], out=middlec);
    // And(a=middlec, b=notsel0, out=finalc);

    // And(a=d, b=sel[1], out=middled);
    // And(a=middled, b=sel[0], out=finald);

    // Or(a=finala, b=finalb, out=anb);
