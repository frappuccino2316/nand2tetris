CHIP Multi4Mux {
    /**
    * 00ならaを出力
    * 01ならbを出力
    * 10ならcを出力
    * 11ならdを出力
    */
    IN a, b, c, d, sel[2];
    OUT out;
    
    PARTS:
    // Muxを使う
    Mux(a=a, b=b, sel=sel[0], out=r1);
    Mux(a=c, b=d, sel=sel[0], out=r2);
    Mux(a=r1, b=r2, sel=sel[1], out=out);

